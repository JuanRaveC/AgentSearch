<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Frame" id="Frame_1">
    <property name="height">200</property>
    <property name="width">400</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">4</property>
      <property name="sticky">n</property>
      <rows>
        <row id="0">
          <property name="minsize">50</property>
          <property name="pad">50</property>
          <property name="weight">6</property>
        </row>
        <row id="1">
          <property name="minsize">10</property>
          <property name="pad">5</property>
        </row>
        <row id="2">
          <property name="minsize">50</property>
          <property name="pad">10</property>
        </row>
        <row id="3">
          <property name="minsize">50</property>
          <property name="pad">50</property>
        </row>
      </rows>
      <columns>
        <column id="0">
          <property name="minsize">50</property>
          <property name="pad">50</property>
        </column>
      </columns>
    </layout>
    <child>
      <object class="ttk.Label" id="Label_1">
        <property name="compound">top</property>
        <property name="font">TkDefaultFont</property>
        <property name="padding">10</property>
        <property name="takefocus">false</property>
        <property name="text" translatable="yes">SISTEMA DE CONSULTA UNIFICADO EN CATALOGOS WEB</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">n</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Entry" id="search_input">
        <property name="cursor">arrow</property>
        <property name="exportselection">true</property>
        <property name="justify">center</property>
        <property name="takefocus">true</property>
        <property name="textvariable">string:search_input</property>
        <property name="width">50</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="search_button">
        <property name="command">search_on_click</property>
        <property name="compound">top</property>
        <property name="cursor">arrow</property>
        <property name="takefocus">true</property>
        <property name="text" translatable="yes">Buscar</property>
        <property name="textvariable">int:search_button</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="Label_2">
        <property name="text" translatable="yes">Por favor ingresa la palabra clave a buscar:</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
